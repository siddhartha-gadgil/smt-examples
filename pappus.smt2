(set-logic AUFNIRA)
(declare-fun b_scale () Real)
(declare-fun c_scale () Real)
(declare-fun ax () Real)
(declare-fun ay () Real)
(declare-fun Bx () Real)
(declare-fun Cx () Real)
(declare-fun Px () Real)
(declare-fun Py () Real)
(declare-fun Qx () Real)
(declare-fun Qy () Real)
(declare-fun Rx () Real)
(declare-fun Ry () Real)
(assert (< 1.0 b_scale))
(assert (< b_scale c_scale))
(assert (< 0.0 ax))
(assert (< 0.0 ay))
(assert (= (* (- (* ax c_scale) ax) (- (* ay b_scale) ay))
   (* (- (* ax b_scale) ax) (- (* ay c_scale) ay))))
(assert (= (* (- Cx 1.0) (to_real (- 0 0)))
   (* (- Bx 1.0) 0.0)))
(assert (= (* (- Bx ax) (- Py ay)) (* (- Px ax) (- 0.0 ay))))
(assert (= (* (- (* ax b_scale) 1.0) (- Py 0.0))
   (* (- Px 1.0) (- (* ay b_scale) 0.0))))
(assert (= (* (- Cx ax) (- Qy ay)) (* (- Qx ax) (- 0.0 ay))))
(assert (= (* (- (* ax c_scale) 1.0) (- Qy 0.0))
   (* (- Qx 1.0) (- (* ay c_scale) 0.0))))
(assert (= (* (- Cx (* ax b_scale)) (- Ry (* ay b_scale)))
   (* (- Rx (* ax b_scale)) (- 0.0 (* ay b_scale)))))
(assert (= (* (- (* ax c_scale) Bx) (- Ry 0.0))
   (* (- Rx Bx) (- (* ay c_scale) 0.0))))
(assert (not (= (* (- Rx Px) (- Qy Py)) (* (- Qx Px) (- Ry Py)))))
(check-sat)
