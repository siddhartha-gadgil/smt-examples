(set-logic AUFNIRA)
(set-option :produce-models true)
(set-option :produce-proofs true)
(declare-fun n() Int)
(assert (> n 3))
(assert (<= (* n n) 30))
(assert (>= (* n 3) 10))
(assert (= (mod n 2) 1))
(check-sat)
(get-value (n))
(declare-fun m() Int)
(assert (= (mod m 2) 1))
(assert (= (mod (+ m 1) 2) 1))
(check-sat)
(get-proof)